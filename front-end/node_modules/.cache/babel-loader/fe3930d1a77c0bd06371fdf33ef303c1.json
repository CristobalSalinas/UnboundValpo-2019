FS;AAG3Bd,EAAAA,MAAM,EAANA;AAH2B,CAAd,CAAf;AAMAS,MAAM,CAACM,UAAP,CAAkB,UAACC,EAAD,EAAIC,IAAJ,EAASC,IAAT,EAAiB;AACjC,MAAIF,EAAE,CAACG,OAAH,CAAWC,IAAX,CAAgB,UAAAC,MAAM;AAAA,WAAIA,MAAM,CAACjB,IAAP,CAAYI,KAAhB;AAAA,GAAtB,CAAJ,EAAkD;AAAE;AAClDU,IAAAA,IAAI;AACL,GAFD,MAEO,IAAIxB,KAAK,CAAC4B,KAAN,CAAYC,OAAZ,IAAuB7B,KAAK,CAAC4B,KAAN,CAAYC,OAAZ,CAAoBC,GAApB,IAA2B,eAAtD,EAAuE;AAC5E,QAAIR,EAAE,CAACG,OAAH,CAAWC,IAAX,CAAgB,UAAAC,MAAM;AAAA,aAAIA,MAAM,CAACjB,IAAP,CAAYC,aAAhB;AAAA,KAAtB,CAAJ,EAA0D;AACtDa,MAAAA,IAAI;AACP;AACF,GAJM,MAIA,IAAIxB,KAAK,CAAC4B,KAAN,CAAYC,OAAZ,IAAuB7B,KAAK,CAAC4B,KAAN,CAAYC,OAAZ,CAAoBC,GAApB,IAA2B,cAAtD,EAAsE;AAC3E,QAAIR,EAAE,CAACG,OAAH,CAAWC,IAAX,CAAgB,UAAAC,MAAM;AAAA,aAAIA,MAAM,CAACjB,IAAP,CAAYG,YAAhB;AAAA,KAAtB,CAAJ,EAAyD;AACvDW,MAAAA,IAAI;AACL;AACF,GAJM,MAIA,IAAIxB,KAAK,CAAC4B,KAAN,CAAYC,OAAZ,IAAuB7B,KAAK,CAAC4B,KAAN,CAAYC,OAAZ,CAAoBC,GAApB,IAA2B,QAAtD,EAAgE;AACrE,QAAIR,EAAE,CAACG,OAAH,CAAWC,IAAX,CAAgB,UAAAC,MAAM;AAAA,aAAIA,MAAM,CAACjB,IAAP,CAAYE,MAAhB;AAAA,KAAtB,CAAJ,EAAmD;AACjDY,MAAAA,IAAI;AACL;AACF,GAJM,MAIA;AACLA,IAAAA,IAAI,CAAC;AAAChB,MAAAA,IAAI,EAAC;AAAN,KAAD,CAAJ;AACD;AACF,CAlBD;AAoBA,eAAeO,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport store from '../store/index'\nimport Home from '../views/Home.vue'\nimport Proyecto from '../components/Proyecto.vue'\nimport Login from '../components/Login.vue'\nimport Usuario from '../components/Usuario.vue'\n\nVue.use(VueRouter)\n\nconst routes = [\n  {\n    path: '/',\n    name: 'home',\n    component: Home,\n    meta:{\n      administrador:true,\n      lector:true,\n      asist_social:true\n    }\n  },\n  {\n    path: '/proyecto',\n    name: 'proyecto',\n    component: Proyecto,\n    meta: {\n      administrador: true,\n    }\n  },\n  {\n    path: '/login',\n    name: 'login',\n    component: Login,\n    meta: {\n      libre: true\n    }\n  },\n  {\n    path: '/usuario',\n    name: 'usuario',\n    component: Usuario,\n    meta: {\n      // administrador: true\n      libre:true\n    }\n  },\n]\n\nconst router = new VueRouter({\n  mode: 'history',\n  base: process.env.BASE_URL,\n  routes\n})\n\nrouter.beforeEach((to,from,next) =>{\n  if (to.matched.some(record => record.meta.libre)) { //dando acceso a las rutas que tinene en su archivo \"meta\" = libre\n    next();\n  } else if (store.state.usuario && store.state.usuario.rol == 'Administrador') {\n    if (to.matched.some(record => record.meta.administrador)) {\n        next();\n    }\n  } else if (store.state.usuario && store.state.usuario.rol == 'Asist_Social') {\n    if (to.matched.some(record => record.meta.asist_social)) {\n      next();\n    }\n  } else if (store.state.usuario && store.state.usuario.rol == 'Lector') {\n    if (to.matched.some(record => record.meta.lector)) {\n      next();\n    }\n  } else {\n    next({name:'login'});\n  }\n});\n\nexport default router\n"]}]}